#!/usr/bin/env php
<?php

require_once __DIR__.'/../lib/Util/Constant.php';
require_once LIB_DIR.'/Handler/FileHandler.php';

try {
    $handler = new \Handler\FileHandler();
    $handler->convertDocsToRsts();
} catch (Exception $e) {
    echo $e->getMessage();
    echo $e->getTraceAsString();
    exit(1);
}